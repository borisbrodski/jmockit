      <area shape="rect" coords="778,183,949,210" target="_blank"
         href="http://jmockit.googlecode.com/svn/trunk/www/javadoc/mockit/UsingMocksAndStubs.html"
         title="Indicates one or more classes to be mocked or stubbed out.">

      <area shape="rect" coords="10,225,144,253" target="_blank"
